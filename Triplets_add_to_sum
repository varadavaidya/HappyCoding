def Triplets_Sum(arr,sum):
    m=len(arr)
    for i in range(m):
        for j in range(i+1,m):
            for k in range(j+1,m):
                if arr[i]+arr[j]+arr[k]==sum:
                    return (arr[i],arr[j],arr[k])
                else:
                    pass
    return "No Triplets"




print(Triplets_Sum([1,2,3,4,5],8))
