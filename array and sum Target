def func(nums,target):
    for i in range(len(nums)):
        for j in range(i+1,len(nums)):
            if nums[i]+nums[j]==target:
                result=[i,j]
                return result
            else:
                j=+1
        i=+1

print(func([2,7,11,15],9))
print(func([3,2,4],6))
print(func([3,3],6))
